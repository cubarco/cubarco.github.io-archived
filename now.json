{
  "version": 2,
  "name": "blog",
  "alias": "cub4r.co",
  "builds": [
    { "src": "build.sh", "use": "@now/static-build", "config": { "distDir": "_site" } }
  ],
  "routes": [
    {
      "src": "/gist/(.*)",
      "dest": "https://gist.github.com/$1",
      "headers": {"cache-control": "s-maxage=604800"}
    }
  ]
}


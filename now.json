{
   "version":2,
   "name":"blog",
   "alias":"jekyll.cubarco.org",
   "builds": [
       {
           "src": "build.sh",
           "use": "@now/static-build",
           "config": {"distDir": "_site"}
       }
   ],
   "rewrites":[
      {
         "source":"/gist/(.*)",
         "destination":"https://gist.github.com/$1"
      }
   ],
   "headers":[
      {
         "source":"(.*)",
         "headers":[
            {
               "key":"Access-Control-Allow-Origin",
               "value":"*"
            }
         ]
      }
   ]
}
